<h4>Editing <%= @building.name %></h4>

<%= form_for @building  do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :rent_per_floor %>
  <%= f.text_field :rent_per_floor %>

  <%=f.submit%>
  <% end%>